# Ignore node_modules for both frontend and backend
/frontend/node_modules
/backend/node_modules

# Ignore build directories for frontend and backend (for example, React/Vue build, Node.js dist)
/frontend/build
/frontend/.next
/frontend/dist
/backend/dist
.env

# Ignore environment variable files
# For frontend
/frontend/.env
/frontend/.env.local
/frontend/.env.production
/frontend/.env.development

# For backend
/backend/.env
/backend/.env.local
/backend/.env.production
/backend/.env.development

# Ignore logs and temporary files
logs/
*.log
*.log.*
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE and editor files
/.idea/
/.vscode/
/*.sublime-workspace
*.swp

# OS-specific files
.DS_Store
Thumbs.db

# Ignore coverage directories (if you're using code coverage tools)
coverage/

# Ignore package lock files if you want to ignore specific versions or not lock them
# /frontend/package-lock.json
# /backend/package-lock.json

# Ignore other potentially sensitive files
*.env
*.key
*.pem
